<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="275.0" prefWidth="316.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gridwindow.top.interactiveformuladialog.InteractiveFormulaDialogController">
    <children>
        <VBox layoutX="10.0" layoutY="10.0" spacing="10">
            <ComboBox fx:id="functionComboBox" promptText="Select Function" />
            <VBox fx:id="argumentContainer" spacing="5" />
        </VBox>
      <TextField fx:id="expressionTextField" layoutX="11.0" layoutY="146.0" prefHeight="25.0" prefWidth="259.0" />
         <Label fx:id="resultPreview" layoutX="11.0" layoutY="212.0" text="Result:" />
      <Button fx:id="calculateButton" layoutX="11.0" layoutY="179.0" mnemonicParsing="false" text="Calculate" />
      <Button fx:id="applyButton" layoutX="220.0" layoutY="240.0" mnemonicParsing="false" text="Apply" />
      <Button fx:id="cancelButton" layoutX="10.0" layoutY="240.0" mnemonicParsing="false" text="Cancel" />
    </children>
</AnchorPane>
